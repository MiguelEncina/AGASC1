#!/bin/bash

# Iterar sobre un rango de n√∫meros
for i in {1..9}; do
    python3 zdt3/zdt3.py
    cat <<EOL > metrics.in
2
1
2
zdt3/EVAL10000/P100G100/zdt3_all_popmp100g100_seed0$i.out
100
100
zdt3/zdt3_all_popm.out
100
100
1
EOL
    ./metrics < metrics.in
    mv spacing.out zdt3/RESULTS/P100G100/spacing_NSGAII_p100g100_seed0$i.out
    mv spacing2.out zdt3/RESULTS/P100G100/spacing_ZDT3_p100g100_seed0$i.out
    mv hypervol.out zdt3/RESULTS/P100G100/hypervol_NSGAII_p100g100_seed0$i.out
    mv hypervol2.out zdt3/RESULTS/P100G100/hypervol_ZDT3_p100g100_seed0$i.out
    mv cs.out zdt3/RESULTS/P100G100/cs_NSGAII_p100g100_seed0$i.out
    mv cs2.out zdt3/RESULTS/P100G100/cs_ZDT3_p100g100_seed0$i.out
    mv hvref zdt3/RESULTS/P100G100/hvref_p100g100_seed0$i

done
